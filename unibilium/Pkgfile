# Description: A terminfo parsing library.
# URL:         https://github.com/mauke/unibilium/
# Depends on:

name=unibilium
version=2.1.0
release=1
source=(https://github.com/neovim/$name/archive/v$version.tar.gz)

build() {
	cd $name-$version

	make PREFIX=/usr
	make PREFIX=/usr DESTDIR=$PKG install
}
